<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jbpm="http://drools.org/schema/drools-spring"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
    http://drools.org/schema/drools-spring org/drools/container/spring/drools-spring-1.2.0.xsd">


	<bean id="eval1" class="com.sample.MediclaimProcessModel">
		<!-- Negative case -->
		<property name="userName" value="Sudharshan" />
		<property name="userAge" value="23" />
		<property name="userCoverage" value="true" />
		<property name="userGender" value="M" />
	</bean>
	<bean id="eval2" class="com.sample.MediclaimProcessModel">
		<!-- Positive case -->
		<property name="userName" value="Angel" />
		<property name="userAge" value="23" />
		<property name="userCoverage" value="true" />
		<property name="userGender" value="F" />
	</bean>
</beans>